def insertion_sort(ar):
    for i in range(1, len(ar)): #i=1,2,3,4,5
        j = i-1                 #j = 0,1,2,3,4
        key = ar[i]             #key=4,3,5,6,2
        while (j >= 0) and (ar[j] > key): #j=1,2,3,4 and ar[j]=7,4,3,5,6
           ar[j+1] = ar[j]
           j -= 1
        ar[j+1] = key
