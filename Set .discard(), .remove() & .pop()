n = int(input())
s = set(map(int, input().split()))
N = int(input())

for _ in range(N):
    command = input().split()
    if command[0] == 'pop' and len(s)!=0:
        s.pop()
    elif command[0] == 'remove' and int(command[1]) in s:
        s.remove(int(command[1]))
    elif command[0] == 'discard':
        s.discard(int(command[1]))
print(sum(list(s)))
